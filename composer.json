  "keywords": [
    "xml",
    "php",
    "xmltoolkit"
  ],
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.3"
  },
  "require-dev": {
        "phpunit/phpunit": "^11.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Ottosmops\\Xmltoolkit\\": "src/"
    }
  },
  "autoload-dev": {
